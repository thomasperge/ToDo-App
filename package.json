{
  "name": "todo",
  "version": "1.0.0",
  "description": "ToDoList Electron App",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --icon=assets/icon.ico",
    "package-linux": "electron-packager . ToDo --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icon.ico --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasperge/ToDoListApp.git"
  },
  "author": "Thomas Kauffmant",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomasperge/ToDoListApp/issues"
  },
  "homepage": "https://github.com/thomasperge/ToDoListApp#readme",
  "devDependencies": {
    "chart.js": "^4.0.1",
    "electron": "^21.2.3"
  },
  "dependencies": {
    "custom-electron-titlebar": "^4.1.1",
    "electron-package": "^0.1.0",
    "electron-packager": "^17.1.1",
    "fs": "^0.0.1-security"
  }
}
